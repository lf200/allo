<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.secaicontainerengine.mapper.LogMapper">

    <delete id="deleteByContainers" parameterType="java.util.List">
        DELETE FROM log
        WHERE containerName IN 
        <foreach collection="containers" item="container" open="(" separator="," close=")">
            #{container}
        </foreach>
    </delete>

</mapper>
